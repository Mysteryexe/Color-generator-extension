{
    "manifest_version": 2,
    "name": "Color Generator",
    "description": "Generates random color",
    "author": "Amirali Pourmehdi",
    "version": "1.14",
    "permissions": [
        "storage"
    ],
    "browser_action": {
      "default_icon": "icon128.png",
      "icons": {
         "16": "icon16.png",
         "48": "icon48.png",
        "128": "icon128.png"
      },
      "default_popup": "index.html"
    }
  }